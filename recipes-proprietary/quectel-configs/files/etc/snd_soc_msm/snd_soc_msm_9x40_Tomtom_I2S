}

SectionUseCase."VoLTE" {
Comment "VoLTE use case"
SectionVerb
	Name "VoLTE"

	EnableSequence
		'PRI_MI2S_RX_Voice Mixer VoLTE':1:1
		'VoLTE_Tx Mixer PRI_MI2S_TX_VoLTE':1:1
	EndSequence

	DisableSequence
		'PRI_MI2S_RX_Voice Mixer VoLTE':1:0
		'VoLTE_Tx Mixer PRI_MI2S_TX_VoLTE':1:0
	EndSequence

	# ALSA PCMs
